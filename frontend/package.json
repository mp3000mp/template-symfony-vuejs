{
  "name": "mp3000mp-template-symfony-vuejs",
  "version": "0.3.0",
  "description": "Basic web template",
  "keywords": [
    "symfony",
    "vuejs",
    "template"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "mp3000mp",
      "email": "moussadedijon@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mp3000mp/template-symfony-vuejs"
  },
  "scripts": {
    "serve": "vue-cli-service serve --port 8081",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:ucov": "vue-cli-service test:unit --coverage=true",
    "test:e2e": "vue-cli-service test:e2e --headless --env coverage=false",
    "test:e2ecov": "vue-cli-service test:e2e --headless",
    "test:e2eui": "vue-cli-service test:e2e --env coverage=false",
    "lint": "vue-cli-service lint",
    "lint:style": "stylelint ./src/**/*.{vue,scss}"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/vue-fontawesome": "^3.0.0-3",
    "axios": "^0.21.1",
    "bootstrap": "^5.0.0",
    "core-js": "^3.6.5",
    "cypress": "^6.8.0",
    "register-service-worker": "^1.7.1",
    "vue": "^3.0.0",
    "vue-class-component": "^8.0.0-0",
    "vue-multiselect": "^3.0.0-alpha.2",
    "vue-router": "^4.0.0",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.5",
    "@types/bootstrap": "^5.0.10",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-cypress": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^2.0.0-0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^7.0.0-0",
    "node-sass": "^6.0.0",
    "sass-loader": "^10.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.19.0",
    "stylelint-webpack-plugin": "^3.0.0",
    "typescript": "^4.0.0",
    "vue-jest": "^5.0.0-0"
  },
  "resolutions": {
    "cypress": "^6.8.0"
  },
  "nyc": {
    "extension": [
      ".js",
      ".ts",
      ".vue"
    ],
    "include": [
      "src/**/*.{js,ts,vue}"
    ],
    "exclude": [
      "tests",
      "src/router/*.*",
      "src/*.{js,ts,vue}"
    ],
    "excludeAfterRemap": true,
    "report-dir": "ci/coverage/e2e"
  }
}
